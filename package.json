{
  "name": "telixia-app",
  "version": "1.0.0",
  "description": "Telixia API",
  "main": "dist",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "build": "babel src -s -D -d dist",
    "babel-node": "babel-node --presets=latest",
    "start": "NODE_ENV=development nodemon --exec npm run babel-node -- ./src",
    "serve": "NODE_ENV=production node ./dist",
    "mocha": "mocha --timeout 10000 --compilers js:babel-register",
    "test": "NODE_ENV=test mocha --compilers --no-deprecation js:babel-register --recursive ./test/",
    "kill": "sudo kill $(sudo lsof -t -i:3000)",
    "doc": "rm -rf dist/public && apidoc -i src/ -o dist/public && opn dist/public/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nditah/telixia-backend.git"
  },
  "keywords": [
    "Express",
    "RestAPI"
  ],
  "author": "Winning Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Nditah/telixia-api/issues"
  },
  "homepage": "https://github.com/Nditah/telixia-api#readme",
  "dependencies": {
    "app-root-path": "^2.1.0",
    "aws-sdk": "^2.378.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "chai-http": "^4.0.0",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "express": "^4.16.3",
    "express-force-ssl": "^0.3.2",
    "helmet": "^3.13.0",
    "joi": "^14.3.0",
    "jsonwebtoken": "^8.4.0",
    "log4js": "^3.0.6",
    "morgan": "^1.9.0",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.4",
    "neo-async": "^2.6.0",
    "pm2": "^3.1.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "sequelize": "^4.41.2",
    "sequelize-cli": "^5.3.0",
    "slugify": "^1.3.4",
    "underscore": "^1.9.1",
    "util": "^0.11.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "apidoc": "^0.17.6",
    "apidoc-markdown2": "^0.2.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coffeescript": "^2.3.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6",
    "opn-cli": "^3.1.0",
    "should": "^13.2.1",
    "supertest": "^3.1.0"
  },
  "engines": {
    "node": "10.9.0"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
